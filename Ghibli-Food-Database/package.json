{
  "name": "ghibli-food-database",
  "version": "1.0.0",
  "description": "Database management, administration, and tooling for the Ghibli Food Recipe application",
  "main": "index.js",
  "scripts": {
    "start": "node admin/server.js",
    "dev": "nodemon admin/server.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "create:migration": "sequelize-cli migration:generate --name",
    "create:seed": "sequelize-cli seed:generate --name",
    "backup:create": "node scripts/backup-database.js",
    "backup:restore": "node scripts/restore-database.js",
    "analyze": "node scripts/analyze-database.js",
    "optimize": "node scripts/optimize-database.js",
    "health-check": "node scripts/health-check.js",
    "test:connection": "node scripts/test-connection.js",
    "admin": "node admin/create-admin.js",
    "reset:dev": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "setup": "npm run migrate && npm run seed && npm run admin",
    "monitor": "node monitoring/monitor.js"
  },
  "keywords": [
    "database",
    "postgresql",
    "sequelize",
    "ghibli-food",
    "admin",
    "backup",
    "monitoring"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.3",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2",
    "winston": "^3.8.2",
    "nodemailer": "^6.9.1",
    "commander": "^10.0.0",
    "inquirer": "^9.1.4",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "table": "^6.8.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "archiver": "^5.3.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.5.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}